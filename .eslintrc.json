{
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "parser": "@typescript-eslint/parser",
  "extends": [
    "plugin:@typescript-eslint/recommended",
    "airbnb"
  ],
  "plugins": [
            "@typescript-eslint",
            "react",
            "react-hooks"
    ],
  "rules": {
    "no-use-before-define": 0,
    "import/prefer-default-export": 0,
    "no-undef": 1,
    "react/jsx-filename-extension": [1,
      {
        "extensions": [
          ".tsx"
        ]
      }
    ]
  }
}
