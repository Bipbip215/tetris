{
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "parser": "@typescript-eslint/parser",
  "extends": [
    "plugin:@typescript-eslint/recommended"
  ],
  "plugins": [
    "import",
    "react",
    "react-hooks"
  ],
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "no-use-before-define": 0,
    "import/prefer-default-export": 0,
    "no-undef": 1
  }
}
